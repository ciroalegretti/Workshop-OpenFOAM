FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue alpha.water 0
);

regions
(
cylinderToCell
{
p1 (0.3 0.4 0); //Min
p2 (0.3 0.4 0.15); //Max
radius 0.08;

fieldValues
(
volScalarFieldValue alpha.water 1
);
}
);

   

